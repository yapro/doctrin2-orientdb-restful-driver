{
    "name":        "yapro/doctrin2-orientdb-restful-driver",
    "description": "Doctrin2 PHP RESTful driver for OrientDB graph database.",
    "version":     "0.0.7",
    "type":        "symfony-bundle",
    "keywords":    ["doctrine2", "orientdb", "restful", "driver"],
    "homepage":    "http://github.com/yapro/doctrin2-orientdb-restful-driver",
    "license":     "MIT",

    "authors": [
        {
            "name": "Lebedenko Nikolay",
            "email": "totx@narod.ru",
            "homepage": "http://yapro.ru"
        }
    ],

    "require": {
        "php":                          ">=5.3.3",
        "symfony/framework-bundle":     "~2.1",
        "symfony/security-bundle":      "~2.1"
    },

    "require-dev": {
        "doctrine/orm":                 "~2.2",
        "symfony/validator":            "~2.1"
    },

    "autoload": {
        "psr-0": {
            "OrientDB": ""
        }
    },

    "target-dir": "",

    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
